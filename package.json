{
  "name": "leaflet-labeled-circle",
  "version": "1.0.4",
  "description": "Special type of SVG marker with a label inside and draggable around the anchor point",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "browser": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "_test": "browserify ./test/*.test.js | tap-closer | smokestack | tap-status",
    "test:watch": "vitest",
    "_test:watch": "nodemon --exec 'npm run lint && npm test'",
    "lint": "eslint ./src/",
    "start": "vite dev",
    "_start": "npm run watch & npm run server",
    "watch": "watchify -d demo/js/index.js -o demo/js/build.js -v",
    "server": "http-server -p 3002",
    "build-js": "browserify -v -d --external leaflet ./index.js -o dist/L.LabeledCircle-src.js",
    "compress": "uglifyjs ./dist/L.LabeledCircle-src.js -o ./dist/L.LabeledCircle.js -m --comments",
    "build": "vite build && npm run types",
    "_build": "npm run lint && npm run build-js && npm test && npm run compress",
    "types": "tsc --d -emitDeclarationOnly --outDir dist/types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w8r/leaflet-labeled-circle.git"
  },
  "keywords": [
    "leaflet",
    "label",
    "circlemarker",
    "svg",
    "draggable"
  ],
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w8r/leaflet-labeled-circle/issues"
  },
  "homepage": "https://github.com/w8r/leaflet-labeled-circle#readme",
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "leaflet": "global:L"
  },
  "dependencies": {
    "leaflet-path-drag": "^1.9.4"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.0.0",
    "browserify-shim": "^3.8.14",
    "eslint": "^4.18.2",
    "faucet": "0.0.1",
    "http-server": "^0.11.1",
    "leaflet": "^1.9.4",
    "nodemon": "^1.19.4",
    "prosthetic-hand": "^1.3.1",
    "smokestack": "^3.6.0",
    "tap-closer": "^1.0.0",
    "tap-status": "^1.0.1",
    "tape": "^4.8.0",
    "typescript": "^5.3.3",
    "uglify-js": "^3.3.10",
    "vite": "^5.1.2",
    "vitest": "^1.2.2",
    "watchify": "^3.11.1"
  }
}
